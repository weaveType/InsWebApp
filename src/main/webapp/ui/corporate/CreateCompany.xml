// 로고 파일 변경 이벤트
scwin.logo_upload_onchange = function(e) {
    var upload = logo_upload;
    var files = upload.getFileList();
    
    if (files && files.length > 0) {
        var file = files[0];
        var fileName = file.name;
        var fileSize = (file.size / 1024).toFixed(1); // KB 단위
        
        // 파일 정보 표시
        document.querySelector('.logo-file-info').textContent = fileName + ' (' + fileSize + 'KB)';
        
        // 미리보기 컨테이너 표시
        document.querySelector('.logo-preview-container').style.display = 'block';
        
        // 파일이 이미지인 경우 미리보기 표시
        if (file.type && file.type.startsWith('image/')) {
            var reader = new FileReader();
            reader.onload = function(e) {
                var previewImg = document.querySelector('.logo-preview-image');
                if (previewImg) {
                    previewImg.src = e.target.result;
                } else {
                    // 이미지 엘리먼트가 없으면 생성
                    var img = document.createElement('img');
                    img.className = 'logo-preview-image';
                    img.src = e.target.result;
                    document.querySelector('.logo-preview-container').appendChild(img);
                }
            };
            reader.readAsDataURL(file);
        }
        
        alert("로고 파일이 선택되었습니다: " + fileName);
    }
};

