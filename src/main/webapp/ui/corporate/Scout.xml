scwin.updateTotalCount = function() {
    var totalCount = developer_list.getTotalRow();
    totalCount_txt.setValue("총 " + totalCount + "명의 개발자");
};

// 선택된 개발자 수 업데이트 (안전한 방법)
scwin.updateSelectedCount = function() {
    try {
        var checkedCount = 0;
        var totalRows = developer_list.getTotalRow();
        
        // 각 행을 순회하면서 체크 상태 확인
        for (var i = 0; i < totalRows; i++) {
            var isChecked = developer_list.getCellData(i, "chk");
            if (isChecked === "Y" || isChecked === true || isChecked === "true") {
                checkedCount++;
            }
        }
        
        selectedCount.setValue(checkedCount + "명 선택됨");
    } catch (e) {
        console.error("선택 개수 업데이트 오류:", e);
        selectedCount.setValue("0명 선택됨");
    }
};

