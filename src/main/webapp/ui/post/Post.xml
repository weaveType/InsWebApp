		btn_delete.show("");    // 삭제 버튼 생성
		
		spn_registerFlag.setValue("수정");
	};

	/**
	 * 저장 버튼 클릭 이벤트
	 * @date
	 * @memberOf
	 * @param 
	 * @returns
	 * @author 
	 * @example
	 */	
	scwin.btn_save_onclick = function(e) {
		
		// 유효성 체크
		var valInfo = [
			{ id: "jobPostingId", mandatory: true }
           
		];
		
		if ($c.data.validateGroup(grp_postInfo, valInfo) === true) {
			var promise = $c.win.confirm("저장하시겠습니까?");
			promise.then(function(result) {  
				if (result){
					$c.sbm.execute(sbm_insertPostVo);
				}
			})
		} 	
	};

	/**
