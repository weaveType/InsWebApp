<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" 
    xmlns:xf="http://www.w3.org/2002/xforms">
    
    <head meta_screenName="기업 리스트">
        <w2:type>COMPONENT</w2:type>
        <w2:buildDate/>
        <w2:MSA/>
        <xf:model>
            <w2:dataCollection baseNode="map">
                <w2:dataList baseNode="list" repeatNode="map" id="company_list" saveRemovedData="true">
                    <w2:columnInfo>
                        <w2:column dataType="text" name="기업ID" id="company_id"></w2:column>
                        <w2:column dataType="text" name="기업명" id="company_name"></w2:column>
                        <w2:column dataType="text" name="업종" id="industry"></w2:column>
                        <w2:column dataType="text" name="위치" id="location"></w2:column>
                        <w2:column dataType="text" name="기업규모" id="company_size"></w2:column>
                        <w2:column dataType="text" name="로고URL" id="logo_url"></w2:column>
                        <w2:column dataType="text" name="기업소개" id="description"></w2:column>
                        <w2:column dataType="text" name="채용중인공고수" id="active_jobs"></w2:column>
                        <w2:column dataType="text" name="평점" id="rating"></w2:column>
                        <w2:column dataType="text" name="복리후생" id="benefits"></w2:column>
                    </w2:columnInfo>
                </w2:dataList>
            </w2:dataCollection>
            <w2:workflowCollection/>
        </xf:model>
        <w2:layoutInfo/>
        <w2:publicInfo method=""/>
        
        <style type="text/css"><![CDATA[
            /* 기본 설정 */
            * {
                box-sizing: border-box;
            }

            body {
                margin: 0;
                padding: 0;
                font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif;
                background-color: #ffffff;
                line-height: 1.6;
            }

            .main-wrap {
                width: 100%;
                background-color: #ffffff;
            }


            /* 햄버거 메뉴 */
            .hamburger-menu {
                width: 24px;
                height: 24px;
                cursor: pointer;
                display: flex;
                flex-direction: column;
                justify-content: center;
                align-items: center;
                padding: 4px;
                border-radius: 4px;
                transition: background-color 0.2s ease;
            }

            .hamburger-menu:hover {
                background-color: #f5f5f5;
            }

            .hamburger-line {
                width: 16px;
                height: 2px;
                background-color: #333;
                margin: 1px 0;
                transition: 0.3s;
                border-radius: 1px;
            }

            .nav-menu {
                display: inline-block;
                vertical-align: middle;
            }

            .nav-menu xf:image {
                vertical-align: middle;
                max-height: 34px;
            }

            .nav-link {
                font-size: 14px;
                color: #333;
                text-decoration: none;
                cursor: pointer;
                margin-right: 20px;
                vertical-align: middle;
            }

            .search-container {
                display: inline-block;
                background-color: #f5f5f5;
                border-radius: 20px;
                padding: 6px 15px;
                margin-right: 10px;
                vertical-align: middle;
            }

            .search-input {
                border: none;
                background: transparent;
                width: 150px;
                outline: none;
                font-size: 13px;
                height: 24px;
                vertical-align: middle;
            }

            .search-btn {
                border: none;
                background: none;
                cursor: pointer;
                margin-left: 8px;
                font-size: 14px;
                vertical-align: middle;
            }

            .btn-login {
                background-color: #fff;
                border: 1px solid #ddd;
                padding: 6px 12px;
                border-radius: 4px;
                cursor: pointer;
                font-size: 12px;
                margin-right: 8px;
                height: 32px;
                vertical-align: middle;
            }

            .btn-enterprise {
                background-color: #333;
                color: #fff;
                border: none;
                padding: 6px 12px;
                border-radius: 4px;
                cursor: pointer;
                font-size: 12px;
                height: 32px;
                vertical-align: middle;
            }

            .clearfix::after {
                content: "";
                display: table;
                clear: both;
            }

            /* 필터 섹션 */
            .filter-section {
                background-color: #ffffff;
                border-bottom: 1px solid #e8e8e8;
                padding: 16px 20px;
            }

            .main-filters {
                display: flex;
                align-items: center;
                gap: 16px;
                margin-bottom: 16px;
            }

            .filter-dropdown {
                position: relative;
                display: flex;
                align-items: center;
                gap: 8px;
                padding: 12px 16px;
                border: 1px solid #e1e2e3;
                border-radius: 25px;
                background: white;
                cursor: pointer;
                font-size: 14px;
                font-weight: 600;
                transition: all 0.2s;
            }

            .filter-dropdown:hover {
                border-color: #36f;
            }

            .filter-dropdown.active {
                border-color: #36f;
                color: #36f;
            }

            .dropdown-arrow {
                width: 12px;
                height: 12px;
                transition: transform 0.2s;
            }

            .filter-tags {
                display: flex;
                align-items: center;
                gap: 8px;
                flex-wrap: wrap;
            }

            .filter-tag {
                display: flex;
                align-items: center;
                gap: 4px;
                padding: 8px 12px;
                background-color: #f8f9fa;
                border: 1px solid #e1e2e3;
                border-radius: 18px;
                font-size: 13px;
                color: #333;
                cursor: pointer;
                transition: all 0.2s;
                white-space: nowrap;
            }

            .filter-tag:hover {
                border-color: #36f;
                color: #36f;
            }

            .filter-tag.active {
                background-color: #36f;
                border-color: #36f;
                color: white;
            }

            .filter-tag.highlight {
                background-color: #fff4e6;
                border-color: #ffa940;
                color: #d48806;
            }

            /* 사이드 메뉴 */
            .side-menu {
                position: fixed;
                top: 70px;
                left: -300px;
                width: 280px;
                height: calc(100vh - 70px);
                background-color: white;
                box-shadow: 2px 0 10px rgba(0,0,0,0.1);
                transition: left 0.3s ease;
                z-index: 1000;
                overflow-y: auto;
                padding: 20px;
            }

            .side-menu.open {
                left: 0;
            }

            .side-menu-overlay {
                position: fixed;
                top: 70px;
                left: 0;
                width: 100%;
                height: calc(100vh - 70px);
                background-color: rgba(0,0,0,0.5);
                display: none;
                z-index: 999;
            }

            .side-menu-overlay.open {
                display: block;
            }

            .side-menu-title {
                font-size: 18px;
                font-weight: bold;
                color: #333;
                margin-bottom: 20px;
                padding-bottom: 15px;
                border-bottom: 1px solid #e0e0e0;
            }

            .side-menu-section {
                margin-bottom: 25px;
            }

            .side-menu-section-title {
                font-size: 14px;
                font-weight: 600;
                color: #666;
                margin-bottom: 10px;
            }

            .side-menu-item {
                display: block;
                padding: 12px 16px;
                margin-bottom: 2px;
                color: #333;
                text-decoration: none;
                border-radius: 6px;
                transition: background-color 0.2s ease;
                font-size: 14px;
                cursor: pointer;
            }

            .side-menu-item:hover {
                background-color: #f8f9fa;
                color: #007bff;
            }

            /* 메인 컨텐츠 */
            .main-content {
                max-width: 1060px;
                margin: 0 auto;
                padding: 24px 20px;
            }

            /* 적극 채용 중인 회사 섹션 */
            .active-hiring-section {
                margin-bottom: 40px;
            }

            .section-header {
                display: flex;
                align-items: center;
                gap: 8px;
                margin-bottom: 20px;
            }

            .fire-icon {
                color: #ff6b35;
                font-size: 18px;
            }

            .section-title {
                font-size: 18px;
                font-weight: 600;
                color: #333;
            }

            .help-icon {
                color: #999;
                font-size: 16px;
                cursor: pointer;
            }

            .company-cards {
                display: grid;
                grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
                gap: 16px;
                margin-bottom: 40px;
            }

            .company-card {
                background: white;
                border-radius: 8px;
                overflow: hidden;
                cursor: pointer;
                transition: all 0.2s ease;
                border: 1px solid #e8e8e8;
            }

            .company-card:hover {
                transform: translateY(-2px);
                box-shadow: 0 8px 20px rgba(0,0,0,0.1);
            }

            .company-image {
                height: 120px;
                background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
                display: flex;
                align-items: center;
                justify-content: center;
                color: white;
                font-size: 16px;
                font-weight: 600;
                position: relative;
            }

            .company-card:nth-child(1) .company-image {
                background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            }

            .company-card:nth-child(2) .company-image {
                background: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
            }

            .company-card:nth-child(3) .company-image {
                background: linear-gradient(135deg, #4facfe 0%, #00f2fe 100%);
            }

            .company-card:nth-child(4) .company-image {
                background: linear-gradient(135deg, #43e97b 0%, #38f9d7 100%);
            }

            .company-card:nth-child(5) .company-image {
                background: linear-gradient(135deg, #fa709a 0%, #fee140 100%);
            }

            .company-info {
                padding: 16px;
            }

            .company-name {
                font-size: 16px;
                font-weight: 600;
                color: #333;
                margin-bottom: 4px;
            }

            .company-positions {
                font-size: 14px;
                color: #666;
            }

            /* 채용공고 섹션 */
            .job-section {
                margin-bottom: 40px;
            }

            .job-cards {
                display: grid;
                grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
                gap: 20px;
            }

            .job-card {
                background: white;
                border-radius: 8px;
                overflow: hidden;
                cursor: pointer;
                transition: all 0.2s ease;
                border: 1px solid #e8e8e8;
                position: relative;
            }

            .job-card:hover {
                transform: translateY(-2px);
                box-shadow: 0 8px 20px rgba(0,0,0,0.1);
            }

            .job-image {
                height: 160px;
                background: #f8f9fa;
                display: flex;
                align-items: center;
                justify-content: center;
                position: relative;
                overflow: hidden;
            }

            .bookmark-btn {
                position: absolute;
                top: 12px;
                right: 12px;
                width: 32px;
                height: 32px;
                background: rgba(0,0,0,0.5);
                border: none;
                border-radius: 4px;
                color: white;
                cursor: pointer;
                display: flex;
                align-items: center;
                justify-content: center;
                transition: all 0.2s;
            }

            .bookmark-btn:hover {
                background: rgba(0,0,0,0.7);
            }

            .job-badge {
                position: absolute;
                top: 12px;
                left: 12px;
                background: #333;
                color: white;
                padding: 4px 8px;
                border-radius: 4px;
                font-size: 11px;
                font-weight: 500;
            }

            .job-content {
                padding: 16px;
            }

            .job-title {
                font-size: 16px;
                font-weight: 600;
                color: #333;
                margin-bottom: 8px;
                line-height: 1.4;
            }

            .job-company {
                font-size: 14px;
                color: #666;
                margin-bottom: 4px;
            }

            .job-location {
                font-size: 13px;
                color: #999;
                margin-bottom: 8px;
            }

            .job-reward {
                font-size: 12px;
                color: #00a8cc;
                font-weight: 500;
            }

            /* 반응형 디자인 */
            @media (max-width: 1024px) {
                .nav-link {
                    margin-right: 15px;
                    font-size: 13px;
                }
                
                .search-input {
                    width: 120px;
                }

                .filter-tags {
                    flex-wrap: wrap;
                }

                .company-cards {
                    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
                }

                .job-cards {
                    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
                }
            }

            @media (max-width: 768px) {
                
                .nav-menu xf:image {
                    width: 100px !important;
                    height: 28px !important;
                }
                
                .nav-link {
                    display: none;
                }
                
                .search-input {
                    width: 100px;
                    font-size: 12px;
                    height: 20px;
                }
                
                .btn-login, .btn-enterprise {
                    padding: 5px 8px;
                    font-size: 11px;
                    height: 28px;
                }

                .side-menu {
                    top: 60px;
                    height: calc(100vh - 60px);
                }

                .side-menu-overlay {
                    top: 60px;
                    height: calc(100vh - 60px);
                }

                .main-content {
                    padding: 16px;
                }

                .filter-tags {
                    overflow-x: auto;
                    flex-wrap: nowrap;
                    padding-bottom: 8px;
                }

                .company-cards {
                    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
                    gap: 12px;
                }

                .job-cards {
                    grid-template-columns: 1fr;
                    gap: 16px;
                }
            }

            @media (max-width: 480px) {
                .filter-section {
                    padding: 12px 16px;
                }

                .main-filters {
                    flex-direction: column;
                    align-items: stretch;
                    gap: 12px;
                }

                .company-cards {
                    grid-template-columns: 1fr;
                }
            }
        ]]></style>
        
        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function () {
    // 페이지 로드 시 초기화
    console.log("원티드 스타일 기업 리스트 페이지 로드");
    
    // 검색 이벤트 바인딩
    setTimeout(function() {
        var searchInput = document.getElementById('searchInput');
        var searchBtn = document.getElementById('btnSearch');
        
        if (searchInput) {
            searchInput.addEventListener('keydown', function(e) {
                if (e.keyCode === 13) {
                    scwin.performSearch();
                }
            });
        }
        
        if (searchBtn) {
            searchBtn.addEventListener('click', function() {
                scwin.performSearch();
            });
        }
    }, 100);
};

// 헤더 이벤트들
scwin.weaveTypeLogo_onclick = function() {
    $p.url("../common/weaveTypeMain.xml");
};

// 검색 실행
scwin.performSearch = function() {
    var searchInput = document.getElementById('searchInput');
    if (!searchInput) {
        alert("검색창을 찾을 수 없습니다.");
        return;
    }
    
    var searchText = searchInput.value;
    if (searchText && searchText.trim() !== "") {
        alert("'" + searchText + "'를 검색합니다. (기업/직무 검색 기능)");
        console.log("검색어:", searchText);
    } else {
        alert("검색어를 입력해주세요.");
    }
};

// 기존 함수들과의 호환성을 위해 유지
scwin.searchInput_onkeypress = function(e) {
    if (e.keyCode === 13) {
        scwin.performSearch();
    }
};

scwin.btnSearch_onclick = function() {
    scwin.performSearch();
};

// 필터 이벤트들
scwin.filter_dropdown_onclick = function(filterType) {
    console.log('필터 드롭다운 클릭:', filterType);
    // 실제로는 드롭다운 메뉴 표시
};

scwin.filter_tag_onclick = function(tagName) {
    console.log('필터 태그 클릭:', tagName);
    // 태그 활성화/비활성화 토글
    var tag = event.target;
    tag.classList.toggle('active');
};

// 기업 카드 클릭
scwin.company_card_onclick = function(companyId) {
    console.log('기업 카드 클릭:', companyId);
    $p.url("PostDetail.xml");
};

// 채용공고 카드 클릭
scwin.job_card_onclick = function(jobId) {
    console.log('채용공고 카드 클릭:', jobId);
    $p.url("PostDetail.xml");
};

// 북마크 버튼 클릭
scwin.bookmark_onclick = function(e, jobId) {
    e.stopPropagation(); // 카드 클릭 이벤트 방지
    console.log('북마크 클릭:', jobId);
    
    var btn = event.target;
    if (btn.innerHTML === '������') {
        btn.innerHTML = '❤️';
    } else {
        btn.innerHTML = '������';
    }
};

// 햄버거 메뉴 토글
scwin.toggleSideMenu = function() {
    var sideMenu = document.getElementById('sideMenu');
    var overlay = document.getElementById('sideMenuOverlay');
    
    if (sideMenu.classList.contains('open')) {
        scwin.closeSideMenu();
    } else {
        sideMenu.classList.add('open');
        overlay.classList.add('open');
    }
};

scwin.closeSideMenu = function() {
    var sideMenu = document.getElementById('sideMenu');
    var overlay = document.getElementById('sideMenuOverlay');
    
    sideMenu.classList.remove('open');
    overlay.classList.remove('open');
};

// 내 정보 페이지로 이동
scwin.goToUserDetail = function() {
    $p.url("../user/UserDetail.xml");
    scwin.closeSideMenu();
};

// 기업 등록 페이지로 이동
scwin.goToCompanyRegister = function() {
    alert("기업 등록 페이지로 이동합니다.");
    scwin.closeSideMenu();
};
]]></script>
    </head>
    
    <body ev:onpageload="scwin.onpageload">
        <xf:group class="main-wrap">
          
            <!-- 사이드 메뉴 오버레이 -->
            <div class="side-menu-overlay" id="sideMenuOverlay" ev:onclick="scwin.closeSideMenu()"></div>
            
            <!-- 사이드 메뉴 -->
            <nav class="side-menu" id="sideMenu">
                <w2:textbox class="side-menu-title" label="메뉴"></w2:textbox>
                
                <div class="side-menu-section">
                    <w2:textbox class="side-menu-section-title" label="매칭 서비스"></w2:textbox>
                    <w2:anchor class="side-menu-item" ev:onclick="scwin.menu1_onclick">
                        <xf:label><![CDATA[💼 매칭]]></xf:label>
                    </w2:anchor>
                    <w2:anchor class="side-menu-item" ev:onclick="scwin.menu2_onclick">
                        <xf:label><![CDATA[🧠 MBTI 검사]]></xf:label>
                    </w2:anchor>
                    <w2:anchor class="side-menu-item" style="background-color:#e3f2fd;color:#1976d2;font-weight:600;">
                        <xf:label><![CDATA[🏢 기업 둘러보기 (현재 페이지)]]></xf:label>
                    </w2:anchor>
                </div>
                
                <div class="side-menu-section">
                    <w2:textbox class="side-menu-section-title" label="개인 서비스"></w2:textbox>
                    <w2:anchor class="side-menu-item" ev:onclick="scwin.goToUserDetail()">
                        <xf:label><![CDATA[👤 내 정보]]></xf:label>
                    </w2:anchor>
                    <w2:anchor class="side-menu-item">
                        <xf:label><![CDATA[📝 지원현황]]></xf:label>
                    </w2:anchor>
                    <w2:anchor class="side-menu-item">
                        <xf:label><![CDATA[❤️ 관심기업]]></xf:label>
                    </w2:anchor>
                    <w2:anchor class="side-menu-item">
                        <xf:label><![CDATA[🔖 북마크]]></xf:label>
                    </w2:anchor>
                </div>
                
                <div class="side-menu-section">
                    <w2:textbox class="side-menu-section-title" label="기업 서비스"></w2:textbox>
                    <w2:anchor class="side-menu-item" ev:onclick="scwin.btnEnterprise_onclick">
                        <xf:label><![CDATA[📊 채용관리 대시보드]]></xf:label>
                    </w2:anchor>
                    <w2:anchor class="side-menu-item" ev:onclick="scwin.goToCompanyRegister()">
                        <xf:label><![CDATA[📋 기업 등록]]></xf:label>
                    </w2:anchor>
                </div>
            </nav>

            <!-- 필터 섹션 -->
            <section class="filter-section">
                <!-- 메인 필터 드롭다운 -->
                <xf:group class="main-filters">
                    <xf:group class="filter-dropdown" ev:onclick="scwin.filter_dropdown_onclick('job_type')">
                        <w2:textbox label="개발 · 개발 전체"></w2:textbox>
                        <w2:span class="dropdown-arrow" label="▼"></w2:span>
                    </xf:group>
                    
                    <xf:group class="filter-dropdown" ev:onclick="scwin.filter_dropdown_onclick('experience')">
                        <w2:textbox label="경력 전체"></w2:textbox>
                        <w2:span class="dropdown-arrow" label="▼"></w2:span>
                    </xf:group>
                    
                    <xf:group style="margin-left: auto; display: flex; gap: 12px;">
                        <w2:textbox label="최신순" style="font-size: 13px; color: #666;"></w2:textbox>
                        <w2:textbox label="추천순" style="font-size: 13px; color: #666;"></w2:textbox>
                        <w2:textbox label="인기순" style="font-size: 13px; color: #666;"></w2:textbox>
                    </xf:group>
                </xf:group>

                <!-- 필터 태그들 -->
                <xf:group class="filter-tags">
                    <w2:span class="filter-tag" label="≥ 태그 전체" ev:onclick="scwin.filter_tag_onclick('tag_all')"></w2:span>
                    <w2:span class="filter-tag" label="한국" ev:onclick="scwin.filter_tag_onclick('korea')"></w2:span>
