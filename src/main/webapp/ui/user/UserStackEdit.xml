                        <w2:key id="selectedStacks" name="선택된기술스택" dataType="text"></w2:key>
                        <w2:key id="searchText" name="검색텍스트" dataType="text"></w2:key>
                    </w2:keyInfo>
                </w2:dataMap>
            </w2:dataCollection>
        </xf:model>
