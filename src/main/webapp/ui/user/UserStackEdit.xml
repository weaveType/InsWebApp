        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {
    // Ï¥àÍ∏∞Ìôî
    dma_techStack.set("selectedStacks", "");
    dma_techStack.set("searchText", "");
    scwin.selectedStacksArray = []; // ÏÑ†ÌÉùÎêú Í∏∞Ïà†Ïä§ÌÉù Î∞∞Ïó¥
    scwin.updateNextButton();
    scwin.updateSelectedStacksDisplay();
};

// Í∏∞Ïà†Ïä§ÌÉù ÏÑ†ÌÉù/Ìï¥Ï†ú
scwin.selectTechStack = function(stackName) {
    var clickedElement = event.target.closest('.tech-item');
    
    if (clickedElement.classList.contains('selected')) {
        // Ïù¥ÎØ∏ ÏÑ†ÌÉùÎêú Í≤ΩÏö∞ - Ìï¥Ï†ú
        clickedElement.classList.remove('selected');
        scwin.removeFromSelectedStacks(stackName);
    } else {
        // ÏÑ†ÌÉùÎêòÏßÄ ÏïäÏùÄ Í≤ΩÏö∞ - Ï∂îÍ∞Ä
        clickedElement.classList.add('selected');
        scwin.addToSelectedStacks(stackName);
    }
    
    scwin.updateSelectedStacksDisplay();
    scwin.updateNextButton();
};

// ÏÑ†ÌÉùÎêú Í∏∞Ïà†Ïä§ÌÉùÏóê Ï∂îÍ∞Ä
scwin.addToSelectedStacks = function(stackName) {
    if (scwin.selectedStacksArray.indexOf(stackName) === -1) {
        scwin.selectedStacksArray.push(stackName);
        dma_techStack.set("selectedStacks", scwin.selectedStacksArray.join(','));
    }
};

// ÏÑ†ÌÉùÎêú Í∏∞Ïà†Ïä§ÌÉùÏóêÏÑú Ï†úÍ±∞
scwin.removeFromSelectedStacks = function(stackName) {
    var index = scwin.selectedStacksArray.indexOf(stackName);
    if (index > -1) {
        scwin.selectedStacksArray.splice(index, 1);
        dma_techStack.set("selectedStacks", scwin.selectedStacksArray.join(','));
    }
};

// ÌÉúÍ∑∏ ÌÅ¥Î¶≠ÏúºÎ°ú Ï†úÍ±∞
scwin.removeStackTag = function(stackName) {
    scwin.removeFromSelectedStacks(stackName);
    
    // UIÏóêÏÑú ÏÑ†ÌÉù Ìï¥Ï†ú
    var techItems = document.querySelectorAll('.tech-item');
    techItems.forEach(function(item) {
        if (item.textContent.trim() === stackName) {
            item.classList.remove('selected');
        }
    });
    
    scwin.updateSelectedStacksDisplay();
    scwin.updateNextButton();
};

// ÏÑ†ÌÉùÎêú Í∏∞Ïà†Ïä§ÌÉù ÌëúÏãú ÏóÖÎç∞Ïù¥Ìä∏
scwin.updateSelectedStacksDisplay = function() {
    var container = document.querySelector('.selected-stacks-container');
    
    if (scwin.selectedStacksArray.length === 0) {
        container.innerHTML = '<div class="empty-message">ÏÑ†ÌÉùÎêú Í∏∞Ïà†Ïä§ÌÉùÏù¥ ÏóÜÏäµÎãàÎã§</div>';
    } else {
        var html = '';
        scwin.selectedStacksArray.forEach(function(stack) {
            html += '<div class="stack-tag" onclick="scwin.removeStackTag(\'' + stack + '\')">' +
                   '<span>' + stack + '</span>' +
                   '<span class="stack-tag-remove">√ó</span>' +
                   '</div>';
        });
        container.innerHTML = html;
    }
};

// Í≤ÄÏÉâ Í∏∞Îä•
scwin.onSearchInput = function() {
    var searchInput = document.querySelector('.search-input');
    var searchText = searchInput.value.toLowerCase();
    var techItems = document.querySelectorAll('.tech-item');
    
    techItems.forEach(function(item) {
        var techName = item.textContent.trim().toLowerCase();
        if (techName.includes(searchText)) {
            item.classList.remove('hidden');
        } else {
            item.classList.add('hidden');
        }
    });
};

// Îã§Ïùå Î≤ÑÌäº ÏÉÅÌÉú ÏóÖÎç∞Ïù¥Ìä∏
scwin.updateNextButton = function() {
    var nextBtn = document.querySelector('.btn-next');
    
    if (scwin.selectedStacksArray.length > 0) {
        nextBtn.classList.add('active');
        nextBtn.disabled = false;
    } else {
        nextBtn.classList.remove('active');
        nextBtn.disabled = true;
    }
};

// Ïù¥Ï†Ñ Î≤ÑÌäº ÌÅ¥Î¶≠
scwin.goBack = function() {
    if (confirm("Ïù¥Ï†Ñ ÌéòÏù¥ÏßÄÎ°ú ÎèåÏïÑÍ∞ÄÏãúÍ≤†ÏäµÎãàÍπå? ÏÑ†ÌÉùÌïòÏã† Í∏∞Ïà†Ïä§ÌÉùÏù¥ ÏÇ¨ÎùºÏßà Ïàò ÏûàÏäµÎãàÎã§.")) {
        $p.url("UserDetail.xml");
    }
};

// Îã§Ïùå Î≤ÑÌäº ÌÅ¥Î¶≠ (ÏôÑÎ£å)
scwin.goNext = function() {
    if (scwin.selectedStacksArray.length === 0) {
        alert("Í∏∞Ïà†Ïä§ÌÉùÏùÑ ÌïòÎÇò Ïù¥ÏÉÅ ÏÑ†ÌÉùÌï¥Ï£ºÏÑ∏Ïöî.");
        return;
    }
    
    // ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥ ÏóÖÎç∞Ïù¥Ìä∏
    console.log("ÏÑ†ÌÉùÎêú Í∏∞Ïà†Ïä§ÌÉù:", scwin.selectedStacksArray);
    alert("Í∏∞Ïà†Ïä§ÌÉùÏù¥ ÏÑ±Í≥µÏ†ÅÏúºÎ°ú ÏóÖÎç∞Ïù¥Ìä∏ÎêòÏóàÏäµÎãàÎã§!\\nÏÑ†ÌÉùÎêú Í∏∞Ïà†: " + scwin.selectedStacksArray.join(', '));
    
    // ÎßàÏù¥ÌéòÏù¥ÏßÄÎ°ú ÎèåÏïÑÍ∞ÄÍ∏∞
    $p.url("UserDetail.xml");
};
]]></script>
    </head>
    
