// 초기화 함수
scwin.init = function() {
    scwin.isAnimating = false;
    scwin.currentSection = 0;
    scwin.touchStartY = 0;
    scwin.touchEndY = 0;
};

// 휠 이벤트 리스너
scwin.addWheelListener = function() {
    var container = document.querySelector('.mbti-container');
    if (container) {
        container.addEventListener('wheel', function(e) {
            e.preventDefault();
            
            if (scwin.isAnimating) return;
            
            if (e.deltaY > 0) {
                // 아래로 스크롤
                scwin.nextSection();
            } else {
                // 위로 스크롤
                scwin.prevSection();
            }
        }, { passive: false });
    }
};

// 키보드 이벤트 리스너
scwin.addKeyboardListener = function() {
    document.addEventListener('keydown', function(e) {
        if (scwin.isAnimating) return;
        
        switch(e.key) {
            case 'ArrowDown':
            case 'PageDown':
                e.preventDefault();
                scwin.nextSection();
                break;
            case 'ArrowUp':
            case 'PageUp':
                e.preventDefault();
                scwin.prevSection();
                break;
            case 'Home':
                e.preventDefault();
                scwin.goToSection(0);
                break;
            case 'End':
                e.preventDefault();
                scwin.goToSection(scwin.mbtiTypes.length - 1);
                break;
        }
    });
};

// 터치 이벤트 리스너 추가
scwin.addTouchListener = function() {
    var container = document.querySelector('.mbti-container');
    if (container) {
        container.addEventListener('touchstart', function(e) {
            scwin.touchStartY = e.touches[0].clientY;
        }, { passive: true });
        
        container.addEventListener('touchend', function(e) {
            scwin.touchEndY = e.changedTouches[0].clientY;
            scwin.handleSwipe();
        }, { passive: true });
    }
};

// 스와이프 처리
scwin.handleSwipe = function() {
    if (scwin.isAnimating) return;
    
    var diff = scwin.touchStartY - scwin.touchEndY;
    var threshold = 50;
    
    if (Math.abs(diff) > threshold) {
        if (diff > 0) {
            // 위로 스와이프 (다음 섹션)
            scwin.nextSection();
        } else {
            // 아래로 스와이프 (이전 섹션)
            scwin.prevSection();
        }
    }
};

// 네비게이션 도트 생성
scwin.createNavigationDots = function() {
    var navContainer = document.querySelector('.mbti-navigation');
    if (!navContainer) return;
    
    scwin.mbtiTypes.forEach(function(mbti, index) {
        var dot = document.createElement('div');
        dot.className = 'nav-dot';
        dot.setAttribute('data-index', index);
        dot.setAttribute('title', mbti.type + ' - ' + mbti.title);
        
        // 도트 내부에 텍스트 추가
        var text = document.createElement('span');
        text.className = 'nav-text';
        text.textContent = mbti.type;
        dot.appendChild(text);
        
        dot.addEventListener('click', function() {
            scwin.goToSection(index);
        });
        
        navContainer.appendChild(dot);
    });
};

// 다음 섹션으로 이동
scwin.nextSection = function() {
    if (scwin.currentSection < scwin.mbtiTypes.length - 1) {
        scwin.goToSection(scwin.currentSection + 1);
    }
};

// 이전 섹션으로 이동
scwin.prevSection = function() {
    if (scwin.currentSection > 0) {
        scwin.goToSection(scwin.currentSection - 1);
    }
};

// 특정 섹션으로 이동
scwin.goToSection = function(index) {
    if (scwin.isAnimating || index === scwin.currentSection) return;
    if (index < 0 || index >= scwin.mbtiTypes.length) return;
    
    scwin.isAnimating = true;
    scwin.currentSection = index;
    
    // 섹션 표시
    scwin.showSection(index);
    
    // 네비게이션 도트 업데이트
    scwin.updateNavigation();
    
    // 애니메이션 종료 후 플래그 해제
    setTimeout(function() {
        scwin.isAnimating = false;
    }, 800);
};

// 섹션 표시
scwin.showSection = function(index) {
    var mbti = scwin.mbtiTypes[index];
    if (!mbti) {
        return;
    }
    
    var sections = document.querySelectorAll('.mbti-section');
    
    // 모든 섹션 숨기기
    sections.forEach(function(section) {
        section.classList.remove('active');
    });
    
    // 현재 섹션 활성화
    var currentSection = sections[index];
    if (currentSection) {
        currentSection.classList.add('active');
    }
    
    // 배경색 변경 (선택사항)
    scwin.updateBackgroundColor(index);
};

// 배경색 업데이트
scwin.updateBackgroundColor = function(index) {
    var colors = [
        '#f5f5f5', '#fafafa', '#f0f0f0', '#f8f8f8',
        '#f5f5f5', '#fafafa', '#f0f0f0', '#f8f8f8',
        '#f5f5f5', '#fafafa', '#f0f0f0', '#f8f8f8',
        '#f5f5f5', '#fafafa', '#f0f0f0', '#f8f8f8'
    ];
    
    var container = document.querySelector('.mbti-wrapper');
    if (container) {
        container.style.backgroundColor = colors[index % colors.length];
    }
};

// 네비게이션 업데이트
scwin.updateNavigation = function() {
    var dots = document.querySelectorAll('.nav-dot');
    dots.forEach(function(dot, index) {
        if (index === scwin.currentSection) {
            dot.classList.add('active');
        } else {
            dot.classList.remove('active');
        }
    });
};
