    if (userInfo != null) {
        // Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú - ÌîÑÎ°úÌïÑ ÏòÅÏó≠ ÌëúÏãú
        grp_login_area.hide();
        grp_profile_area.show();

        // ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥ ÏóÖÎç∞Ïù¥Ìä∏
